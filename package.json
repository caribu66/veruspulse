{
  "name": "veruspulse",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/check-port-and-dev.js",
    "dev:stop": "node scripts/stop-dev-server.js",
    "dev:turbo": "next dev --turbo",
    "build": "next build",
    "build:prod": "NODE_ENV=production next build",
    "build:analyze": "ANALYZE=true npm run build",
    "start": "node scripts/check-port-and-start.js production",
    "start:prod": "NODE_ENV=production next start -p 3000",
    "stop": "node scripts/stop-server.js",
    "deploy:prod": "chmod +x deploy-production-optimized.sh && ./deploy-production-optimized.sh",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "next lint --max-warnings 0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "validate": "npm run type-check && npm run lint:strict && npm run format:check",
    "validate:fix": "npm run lint:fix && npm run format",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run validate && npm run test && npm run test:e2e",
    "analyze": "ANALYZE=true npm run build",
    "prepare": "husky install",
    "precommit": "npm run type-check && npm run lint:strict",
    "clean": "rm -rf .next out node_modules/.cache",
    "clean:all": "npm run clean && rm -rf node_modules",
    "reinstall": "npm run clean:all && npm install",
    "run:migrate": "node scripts/run-migration.js",
    "db:health": "node scripts/check-database-health.js",
    "db:integrity": "node scripts/verify-database-integrity.js",
    "db:test": "node scripts/test-database-operations.js",
    "db:check": "npm run db:health && npm run db:integrity && npm run db:test",
    "test:integrity": "npm run test:integrity:blocks && npm run test:integrity:blockchain",
    "test:integrity:blocks": "node scripts/test-block-integrity.js",
    "test:integrity:blockchain": "node scripts/test-blockchain-info-accuracy.js",
    "test:integrity:verusid": "node scripts/test-verusid-stake-accuracy.js",
    "test:integrity:watch": "TEST_ITERATIONS=10 TEST_INTERVAL=60 node scripts/test-blockchain-info-accuracy.js",
    "test:load": "load-tests/run-load-tests.sh load",
    "test:load:quick": "load-tests/quick-test.sh",
    "test:load:spike": "load-tests/run-load-tests.sh spike",
    "test:load:stress": "load-tests/run-load-tests.sh stress",
    "test:load:all": "load-tests/run-load-tests.sh all",
    "monitor": "load-tests/monitor-during-test.sh",
    "rpc:check": "node scripts/check-rpc-load.js",
    "rpc:monitor": "node scripts/monitor-rpc-usage.js",
    "rpc:verify": "node scripts/verify-rate-limiter-singleton.js",
    "services:status": "bash scripts/status-all-services.sh",
    "services:stop": "bash scripts/stop-all-services.sh",
    "cleanup": "bash scripts/cleanup-on-cursor-exit.sh",
    "zmq:check": "bash scripts/check-zmq-status.sh",
    "audit": "bash scripts/comprehensive-audit.sh",
    "audit:security": "npm audit --production",
    "audit:fix": "npm audit fix",
    "staking:status": "bash scripts/check-staking-status.sh",
    "staking:scan": "bash scripts/continue-staking-scan-optimized.sh",
    "staking:monitor": "bash scripts/monitor-scan.sh",
    "staking:check": "bash scripts/check-scan-status.sh",
    "postinstall": "npm run type-check || echo 'Type checking failed, but continuing...'"
  },
  "dependencies": {
    "@bitgo/utxo-lib": "github:VerusCoin/BitGoJS#utxo-lib-verus",
    "@phosphor-icons/react": "^2.1.7",
    "@playwright/test": "^1.55.1",
    "@radix-ui/react-progress": "^1.1.7",
    "@sentry/nextjs": "^10.17.0",
    "@types/ioredis": "^4.28.10",
    "@types/pg": "^8.15.5",
    "@types/qrcode": "^1.5.5",
    "better-sqlite3": "^11.10.0",
    "clsx": "^2.1.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "echarts": "^5.6.0",
    "echarts-for-react": "^3.0.2",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.0",
    "jose": "^6.1.0",
    "lucide-react": "^0.546.0",
    "next": "15.5.4",
    "next-intl": "^4.4.0",
    "node-fetch": "^2.7.0",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redis": "^5.8.3",
    "tailwind-merge": "^3.3.1",
    "verus-typescript-primitives": "github:VerusCoin/verus-typescript-primitives",
    "verusd-rpc-ts-client": "github:VerusCoin/verusd-rpc-ts-client",
    "verusid-ts-client": "file:.verusid-local",
    "winston": "^3.18.3",
    "zeromq": "6.5.0",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@axe-core/react": "^4.11.0",
    "@next/bundle-analyzer": "^15.5.4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.19",
    "@types/react": "^18.3.25",
    "@types/react-dom": "^18.3.7",
    "@types/redis": "^4.0.10",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "autoprefixer": "^10.4.21",
    "chromatic": "^13.3.2",
    "eslint": "^8.0.0",
    "eslint-config-next": "15.5.4",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^15.2.10",
    "playwright": "^1.55.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "string-width": "^4.2.3",
    "stylelint": "^16.25.0",
    "stylelint-config-standard": "^39.0.1",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "overrides": {
    "base-x": "^4.0.0",
    "bs58": "^5.0.0",
    "axios": "^1.13.0"
  }
}
